    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug React",
            "request": "launch",
            "type": "pwa-chrome",
            "url": "http://localhost:3000",
            "webRoot": "${workspaceFolder}/src"
        },
        
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\src\\components\\Register\\Register.js"
        }
    ]
}